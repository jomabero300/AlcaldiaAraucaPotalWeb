@model AlcaldiaAraucaPortalWeb.Data.Entities.Proc.AffiliateGroupProductive

@{
    ViewData["Title"] = "Grupo Productivo";
}

<h1>Eliminar</h1>

<h3>Realmente desea eliminar esto?</h3>
<div>
    <h4>Affiliate</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.GroupProductiveId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.GroupProductive.GroupProductiveName)
        </dd>

    </dl>
    <form asp-action="ProductiveDelete">
        <input type="hidden" asp-for="AffiliateGroupProductiveId" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Edit" asp-route-id="@Model.AffiliateId" class="btn btn-success"><i class="bi bi-chevron-left"></i> Retornar</a>
    </form>
</div>

