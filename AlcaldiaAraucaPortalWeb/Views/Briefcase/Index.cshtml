@model List<AlcaldiaAraucaPortalWeb.Data.Entities.Acti.Briefcase >;
@{
    ViewData["Title"] = "Index";
}

<link href="~/css/StyleSheet.css" rel="stylesheet" />

<section id="team" class="pb-5">
    <div class="container">
        <h5 class="section-title h1">OUR TEAM</h5>
        <div class="row">
            @foreach (var item in Model)
            {

                <!-- Team member -->
                <div class="col-xs-12 col-sm-6 col-md-4">
                    <div class="image-flip">
                        <div class="mainflip">
                            <div class="frontside">
                                <div class="card">
                                    <div class="card-body text-center">
                                        <p><img class="img-fluid" src="@item.BriefcaseImage" alt="card image"></p>
                                        <h4 class="card-title">@item.BriefcaseTitel</h4>
                                        <p class="card-text">@item.BriefcaseText</p>
                                        <a href="@item.BriefcaseUrl" class="btn btn-primary btn-sm"><i class="bi bi-plus"></i></a>
                                    </div>
                                </div>
                            </div>
                            <div class="backside">
                                <div class="card">
                                    <div class="card-body text-center mt-4">
                                        <h4 class="card-title">@item.BriefcaseTitel</h4>
                                        <p class="card-text">@item.BriefcaseDescrption</p>
                                        <ul class="list-inline">
                                            <li class="list-inline-item">
                                                <a class="social-icon text-xs-center" target="_blank" href="@item.BriefcaseFacebook">
                                                    <i class="bi bi-facebook"></i>
                                                </a>
                                            </li>
                                            <li class="list-inline-item">
                                                <a class="social-icon text-xs-center" target="_blank" href="@item.BriefcaseTwitter">
                                                    <i class="bi bi-twitter"></i>
                                                </a>
                                            </li>
                                            <li class="list-inline-item">
                                                <a class="social-icon text-xs-center" target="_blank" href="@item.BriefcaseSkype">
                                                    <i class="bi bi-skype"></i>
                                                </a>
                                            </li>
                                            <li class="list-inline-item">
                                                <a class="social-icon text-xs-center" target="_blank" href="@item.BriefcaseGoogle">
                                                    <i class="bi bi-google"></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
