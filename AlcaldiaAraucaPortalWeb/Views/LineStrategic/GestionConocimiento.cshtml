@model List<AlcaldiaAraucaPortalWeb.Models.ContentModelView>;
@{
    ViewData["Title"] = "GestionConocimiento";
}

<hr />
<body>
    <section>
        <div class="container-sm">
            <div class="text-center">
                @*<div class="row d-flex justify-content-center text-center g-2">*@
                <div class="row">

                    @foreach (var item in Model)
                    {
                        <div class="col-lg-4 mb-4">
                            <div class="card" style="width:20em;margin:10px;">
                                <a asp-controller="LineStrategic" asp-action="GeneViewHorizontalDetails" asp-route-id="@item.url" class="btn btn-sm btn-primary">
                                    <img src="@Url.Content( @item.img)" class="card-img-top g-2" />
                                </a>
                                <div class="card-body">
                                    <div class="card-title">
                                        <h2>@item.title</h2>
                                    </div>
                                    <div class="card-text">
                                        <p>@item.text</p>
                                    </div>
                                    <a asp-controller="LineStrategic" asp-action="GeneViewHorizontalDetails" asp-route-id="@item.url" class="btn btn-sm btn-primary">Más...</a>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </section>
    <footer>
        <h4 style="color:white;">Avisos legales</h4>
        <a href="#" style="color:white">Política de cookies</a>
    </footer>
</body>


