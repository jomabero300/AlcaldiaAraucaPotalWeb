@model AlcaldiaAraucaPortalWeb.Data.Entities.Gene.PqrsCategory

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar</h1>

<h3>Estás segura de que quieres eliminar esto?</h3>
<div>
    <h4>Category PQRS</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PqrsCategoryName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PqrsCategoryName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="PqrsCategoryId" />
        <button type="submit" class="btn btn-sm btn-warning"><i class="bi bi-trash2-fill">Eliminar</i></button>
        <a asp-action="Index" class="btn btn-sm btn-success"><i class="bi bi-chevron-left"></i> Retornar</a>
    </form>
</div>
