@model AlcaldiaAraucaPortalWeb.Models.ModelsViewRepo.Affiliate.GroupProductiveViewModel

@{
    ViewData["Title"] = "Reporte";
}

<h4>Reporte Grupos productivos</h4>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="AffiliatePqrsGrupoProductivoRdlc">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="GroupProductiveId" class="control-label"></label>
                <select asp-for="GroupProductiveId" class="form-control" asp-items="ViewBag.GroupProductiveId"></select>
                <span asp-validation-for="GroupProductiveId" class="text-danger"></span>
            </div>

            <div class="form-group">
                <button type="submit" class="btn btn-sm btn-primary"><i class="bi bi-file-earmark-pdf"></i>Generar</button>
            </div>

        </form>
    </div>
</div>
